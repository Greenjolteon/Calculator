{
    "name": "Calculator",
    "version": "1.0",
    "description": "Basic Calculator",
    "manifest_version": 3,
    "offline_enabled": true,
    "author": "Gilad Tal",
    "browser_action":{
        "default_popup": "calc.html",
        "default_title": "Calculator"
    },
    "content_scripts": [
        {
            "js": ["scripts/calc.js"]
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'"
}
